function main() {
	turtle->setSpeed(2000)
	turtle->penDown()
	for(var i < 4) {
		turtle->forward(100)
		turtle->rightTurn(90)
	}
	turtle->penUp()
	turtle->forward(100)
	turtle->penDown()
	for(var j < 4) {
		turtle->rightTurn(30)
		turtle->forward(100)
		turtle->rightTurn(120)
		turtle->forward(100)
		turtle->leftTurn(60)
	}
}
